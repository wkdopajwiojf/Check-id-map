---

-- state

local minimized = false
local animing   = false
local lastId    = 0

-- ‡∏Å‡∏•‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡∏à‡∏∞‡∏ñ‡∏π‡∏Å‡∏ã‡πà‡∏≠‡∏ô‡πÄ‡∏ß‡∏•‡∏≤ minimize (‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏° / ‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏û‡∏¥‡∏°‡∏û‡πå / ‡∏õ‡∏∏‡πà‡∏° send / credit)
local uiGroup = {
chatHolder,
messages,
textBox,
sendBtn,
credit,
}


---

-- fade helpers

local function fadeOutGroup(duration)
for _,inst in ipairs(uiGroup) do
if inst:IsA("TextLabel") or inst:IsA("TextButton") or inst:IsA("TextBox") then
tween(inst, duration, {TextTransparency = 1})
end
if inst.BackgroundTransparency ~= nil then
tween(inst, duration, {BackgroundTransparency = 1})
end
end
end

local function fadeInGroup(duration)
-- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏≤‡∏ï‡∏±‡πâ‡∏á‡πÑ‡∏ß‡πâ‡πÄ‡∏î‡∏¥‡∏°‡πÅ‡∏ö‡∏ö‡∏Ñ‡∏á‡∏ó‡∏µ‡πà ‡πÑ‡∏°‡πà‡∏¢‡∏∏‡πà‡∏á‡∏™‡∏µ/‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™‡∏î‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏Ç‡∏≠‡∏á‡∏ô‡∏≤‡∏¢
tween(chatHolder, duration, {
BackgroundTransparency = 0.3;
})
tween(textBox, duration, {
BackgroundTransparency = 0.15;
})
tween(sendBtn, duration, {
BackgroundTransparency = 0.08;
})
tween(messages, duration, {
BackgroundTransparency = 1;
})
tween(credit, duration, {
BackgroundTransparency = 1;
})

for _,inst in ipairs(uiGroup) do  
    if inst:IsA("TextLabel") or inst:IsA("TextButton") or inst:IsA("TextBox") then  
        tween(inst, duration, {TextTransparency = 0})  
    end  
end

end

local function setGroupVisible(v)
for _,inst in ipairs(uiGroup) do
inst.Visible = v
end
end

local function scrollToBottom()
task.wait()
messages.CanvasSize = UDim2.new(0,0,0,layout.AbsoluteContentSize.Y + 10)
messages.CanvasPosition = Vector2.new(
0,
math.max(0, layout.AbsoluteContentSize.Y - messages.AbsoluteWindowSize.Y)
)
end


---

-- chat message display

local function addMessage(author, text, isSelf)
local line = Instance.new("TextLabel")
line.Parent = messages
line.BackgroundTransparency = 1
line.Font = Enum.Font.Gotham
line.TextSize = 18
line.TextWrapped = true
line.AutomaticSize = Enum.AutomaticSize.XY
line.Size = UDim2.new(1,0,0,0)
line.ClipsDescendants = true
line.TextTransparency = 0

local t = getTimeTag()  
if isSelf then  
    line.Text = string.format("%s :%s %s", text, author, t)  
    line.TextXAlignment = Enum.TextXAlignment.Right  
    line.TextColor3 = Color3.fromRGB(100,200,255)  
else  
    line.Text = string.format("%s %s: %s", t, author, text)  
    line.TextXAlignment = Enum.TextXAlignment.Left  
    line.TextColor3 = Color3.fromRGB(255,180,90)  
end  

scrollToBottom()

end


---

-- send to relay

local function sendToDiscord(txt)
local body = HttpService:JSONEncode({
author = player.Name,
text   = txt
})
local ok, res = pcall(function()
return REQUEST({
Url = RELAY_URL .. "/to-discord",
Method = "POST",
Headers = {
["Content-Type"] = "application/json",
["x-relay-key"]   = RELAY_KEY,
},
Body = body
})
end)
if not ok then
addMessage("System","‚ùå Request error", false)
elseif res.StatusCode ~= 200 then
addMessage("System","‚ùå Relay error: "..tostring(res.Body), false)
end
end

local function handleSend()
local txt = textBox.Text
if txt ~= "" then
addMessage(player.Name, txt, true) -- local echo (‡∏ù‡∏±‡πà‡∏á‡πÄ‡∏£‡∏≤ ‡∏™‡∏µ‡∏ü‡πâ‡∏≤ ‡∏Ç‡∏ß‡∏≤)
sendToDiscord(txt)
textBox.Text = ""
end
end

sendBtn.MouseButton1Click:Connect(handleSend)
textBox.FocusLost:Connect(function(enterPressed)
if enterPressed then
handleSend()
end
end)


---

-- poll from relay (with anti-echo)

task.spawn(function()
while true do
local ok, res = pcall(function()
return REQUEST({
Url = RELAY_URL .. "/messages",
Method = "GET",
Headers = {["x-relay-key"]=RELAY_KEY}
})
end)

if ok and res and res.StatusCode == 200 and res.Body then  
        local decoded  
        local good = pcall(function()  
            decoded = HttpService:JSONDecode(res.Body)  
        end)  
        if good and decoded and decoded.ok and type(decoded.messages)=="table" then  
            for _,m in ipairs(decoded.messages) do  
                local msgId  = m.id  
                local author = tostring(m.author or "???")  
                local text   = tostring(m.text or "")  
                if text ~= "" and (not msgId or msgId > lastId) then  
                    -- ‚ùó anti-echo: ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡πá‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏≤‡πÄ‡∏≠‡∏á ‡∏≠‡∏¢‡πà‡∏≤‡πÇ‡∏ä‡∏ß‡πå‡∏ã‡πâ‡∏≥  
                    if author ~= player.Name then  
                        addMessage(author, text, false)  
                    end  
                    if type(msgId) == "number" and msgId > lastId then  
                        lastId = msgId  
                    end  
                end  
            end  
        end  
    end  

    task.wait(1)  
end

end)


---

-- minimize / expand (reuse our stable colors)

local EXPANDED_SIZE  = UDim2.new(0,360,0,260)
local COLLAPSED_SIZE = UDim2.new(0,200,0,50)
local FADE_TIME      = 0.25
local SHRINK_TIME    = 0.3

minBtn.MouseButton1Click:Connect(function()
if animing then return end
animing = true

if not minimized then  
    -- ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏à‡∏∞‡∏¢‡πà‡∏≠  
    fadeOutGroup(FADE_TIME)  
    task.delay(FADE_TIME, function()  
        setGroupVisible(false)  
        local tw = tween(frame, SHRINK_TIME, {Size = COLLAPSED_SIZE})  
        tw.Completed:Connect(function()  
            minimized = true  
            minBtn.Text = "+"  
            animing = false  
        end)  
    end)  
else  
    -- ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏à‡∏∞‡∏Ç‡∏¢‡∏≤‡∏¢‡∏Å‡∏•‡∏±‡∏ö  
    local tw = tween(frame, SHRINK_TIME, {Size = EXPANDED_SIZE})  
    tw.Completed:Connect(function()  
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ç‡∏≠‡∏á‡πÉ‡∏´‡πâ‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏Å‡πà‡∏≠‡∏ô ‡πÅ‡∏•‡πâ‡∏ß‡∏Ñ‡πà‡∏≠‡∏¢ fade in  
        setGroupVisible(true)  

        -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™/‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡∏™‡∏ß‡∏¢ ‡πÜ ‡∏î‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏¥‡∏°  
        fadeInGroup(FADE_TIME)  

        task.delay(FADE_TIME, function()  
            scrollToBottom()  
            minimized = false  
            minBtn.Text = "‚Äì"  
            animing = false  
        end)  
    end)  
end

end)


---

-- cleanup on teleport

player.OnTeleport:Connect(function()
screen:Destroy()
end)


---

-- hello message

addMessage("System","DXD Relay ready ‚úÖ", false)
